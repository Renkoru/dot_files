URxvt*foreground:White
URxvt*background:Black


! fonts
! run "fc-list" for a list of available fonts
/* URxvt*font: xft:Monospace:pixelsize=13 */
/* URxvt*font: xft:Ubuntu Mono:pixelsize=13 */
URxvt*font: xft:Terminus:pixelsize=14:antialias=true:hinting=true
URxvt*boldFont: xft:Terminus:bold:pixelsize=14:antialias=true:hinting=true
/* URxvt*font: xft:DejaVu Sans Mono:pixelsize=13:antialias=true:hinting=true */
/* URxvt*boldFont: xft:DejaVu Sans Mono:bold:pixelsize=13:antialias=true:hinting=true */

/* URxvt*scrollBar: False */
URxvt*scrollBar: true
URxvt*scrollTtyOutput: False
URxvt*scrollTtyKeypress: True
URxvt*secondaryScroll: True
URxvt*saveLines: 12000
URxvt*color4: RoyalBlue
URxvt*color12: RoyalBlue
URxvt*scrollBar_right: false
URxvt*scrollstyle: rxvt
